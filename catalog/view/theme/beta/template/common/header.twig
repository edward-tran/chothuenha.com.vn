<!DOCTYPE html>
<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}" ng-app="myApp">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=edge">

<title>{{ title }}</title>
<base href="{{ base }}" />
{% if description %}
<meta name="description" content="{{ description }}" />
{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}" />
{% endif %}
<script src="catalog/view/theme/beta/js/jquery.js" type="text/javascript"></script>
<script type="text/javascript" src="catalog/view/theme/beta/js/angular/angular.min.js"></script>
<link rel="stylesheet" href="catalog/view/theme/beta/js/angular/angular-material.min.css"/>
<link rel="stylesheet" href="catalog/view/theme/beta/stylesheet/jquery-ui.min.css" />

<link href='catalog/view/theme/beta/js/angular/loading-bar.css' rel='stylesheet' />
<link href="catalog/view/theme/beta/js/angular/bower_components/toastr/toastr.css" type="text/css" rel="stylesheet" />
<link href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen" />
<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="//fonts.googleapis.com/css?family=Open+Sans:400,400i,300,700" rel="stylesheet" type="text/css" /> 
<link href="catalog/view/theme/beta/stylesheet/stylesheet.css?{{ random() }}" rel="stylesheet">

{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
{% endfor %}



{% for link in links %}
<link href="{{ link.href }}" rel="{{ link.rel }}" />
{% endfor %}

<script type="text/javascript" src="catalog/view/theme/beta/js/angular/controller/angular_main.js?{{ random() }}"></script>

{% for script in scripts %}
<script src="{{ script }}" type="text/javascript"></script>
{% endfor %}

{% for analytic in analytics %}
{{ analytic }}
{% endfor %}

<script type="text/javascript">
  var _class = '{{ class }}';
  var base = '{{ base }}';
  var zone_id = {{ zone_id }};
  var district_id = '{{ district_id }}';
  var type_id = {{ type_id }};
  var price_id = {{ price_id }};
  var acreage_id = {{ acreage_id }};
  var bedroom = {{ bedroom }};
  var bathroom = {{ bathroom }};
  var direction_house_id = {{ direction_house_id }};
  var zones = {{(zones|json_encode|raw)}};
</script>

</head>
<body ng-controller="mainController" class="{{ class }}">


<div class="overlay__content"><span class="spinner"></span></div>


<div class="wrap_header">
  <header id="headerNav">
    <div class="container clearfix">
      <div class="row">
        <div class="header_left col-lg-2 col-md-2 col-sm-2 col-xs-2">
          <div class="wrap_logo">
            <a id="logo" href="{{ home }}" style="background: url('{{ logo }}') center no-repeat;    background-size: contain;">
            {{ name }}
            </a>
          </div>
        </div>
        <div class="nav_mobile">
          <md-button class="md-icon-button" ng-click="toggleRight()" ng-hide="isOpenRight()">
          <md-icon md-svg-src="img/icons/menu.svg" aria-label="Close dialog"></md-icon>
          </md-button>
        </div>

        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
          <nav id="navbar_main">
            <md-sidenav class="mobile-menu-aside md-sidenav-right md-whiteframe-4dp md-closed" md-component-id="right">
             

                <div class="header_menu">
                <md-toolbar>
                  <div class="md-toolbar-tools custom_width">
                    <span flex></span>
                    <md-button class="md-icon-button" ng-click="closeMenuRight()">
                      <md-icon md-svg-src="img/icons/ic_close_24px.svg" aria-label="Close dialog"></md-icon>
                    </md-button>
                  </div>
                </md-toolbar>
                <md-toolbar>
                <div class="header clearfix js-reload-html-header">
                <img class="menu-avatar" src="{{ avata }}">
                <div class="menu-user-action">
                  {% if logged %}
                    <md-button href="{{ account }}" class="md-raised md-primary user-action-item">Trang quản lý</md-button>
                    <md-button href="{{ link_add_product }}" class="md-raised md-primary user-action-item">Đăng tin</md-button>
                  {% else %}
                    <md-button class="md-raised md-primary user-action-item" ng-click="showFormLogin($event)">{{ text_login }}</md-button>
                    <md-button class="md-raised md-primary user-action-item" ng-click="showFormReister($event)">{{ text_register }}</md-button>
                    <md-button class="user-action-item add-post" ng-click="showFormLogin($event)">Đăng tin</md-button>
                  {% endif %}
                </div>
              </div>
                </md-toolbar>
              </div>
              
              <md-content layout-padding id="navbar_main_mobile">
                {{ content_menu }}
              </md-content>
             
            </md-sidenav>
          </nav>
        </div>
        
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 user-heder">
          <div id="user-control-header" class="js-reload-html-header">
            
            {% if logged %}
              <span id="append_profile">
              <ul class="user-member clearfix">
                  <li>
                    <div class="member-info" ng-mouseenter="$mdMenu.open()">
                        <a href="">
                            <img class="member-avatar" src="{{ avata }}">
                            <div class="member-name">{{ full_name }}</div>
                        </a>
                        <div class="member-dropdown-content"> 
                            <md-button rel="nofollow" href="{{ account }}">Quản lý tin đăng</md-button>
                            <md-button rel="nofollow" href="{{ payment }}">Nạp tiền vào tài khoản</md-button>
                            <md-button rel="nofollow" href="{{ transaction_payment }}">Lịch sử nạp tiền</md-button>
                            <md-button rel="nofollow" href="{{ transaction_pay }}">Lịch sử thanh toán</md-button>
                            <md-button rel="nofollow" href="{{ account }}">Thông tin cá nhân</md-button>
                            <md-button rel="nofollow" href="{{ password }}">Đổi mật khẩu</md-button>
                            <md-button rel="nofollow" href="{{ wishlist }}">Tin đã lưu</md-button>
                            <md-button rel="nofollow" href="{{ logout }}">{{ text_logout }}</md-button>
                        </div>
                    </div>
                  </li>
              </ul>
              </span>
            {% else %}
            <span id="append_profile">
              <ul class="user-guest clearfix">
                <li>
                  <a rel="nofollow" href="javascript:;" ng-click="showFormReister($event)">{{ text_register }}</a>
                </li>
                <li>
                  <a rel="nofollow" href="javascript:;" ng-click="showFormLogin($event)">{{ text_login }}</a>
                </li>
              </ul>
              </span>
            {% endif %}
            <md-button rel="nofollow" class="btn-dangtin" href="{{ link_add_product }}">Đăng tin</md-button>
            </div>
          </div>
      </div>
    </div>
  </header>


  <main role="main" id="main" class="clearfix">
    <div id="row">
      <div class="col-lg-2 col-md-2 col-sm-2 col-xs-6 left_menu">
      </div>
      <div class="col-lg-10 col-md-10 col-sm-12 col-xs-12">
        <div id="search-box">
          <div class="search-box-inner">
            <div class="search-box-body">
              <div class="search-item location js-filter-show-popup-city">
                <span id="filter_location_name" ng-click="form_fitler_zone($event)">
                  TOÀN QUỐC
                </span>
                <input type="hidden" name="zone_id" value="<@ zone_id @>">
                <input type="hidden" name="district_id" value="<@ district_id @>">
                <span class="delete-item" ng-click="removeFilterv($event)"></span>
              </div>
              <div class="search-item location search-item-type js-filter-show-popup-city" ng-click="form_fitler_types($event)">
                <span>Loại hình</span>
                <input type="hidden" name="type_id" value="<@ type_id @>">
              </div>
              <div class="search-item post-price search-item-price js-filter-show-popup-price" ng-click="form_fitler_price($event)">
                <span>Chọn giá</span>
                <input type="hidden" name="price_id" value="<@ price_id @>">
              </div>
              <div class="search-item post-acreage search-item-acreage js-filter-show-popup-acreage" ng-click="form_fitler_acreage($event)">
                <span>Chọn diện tích</span>
                <input type="hidden" name="acreage_id" value="<@ acreage_id @>">
              </div>
              <div class="search-item post-advance js-filter-show-popup-advanced" ng-click="form_fitler_extension($event)">
                <span>Lọc nâng cao</span>
              </div>
              <div class="search-item submit" ng-click="filter($event)">
                <span>Tìm kiếm</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </main>
</div>